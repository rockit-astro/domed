{
  "daemon": "clasp_dome",
  "log_name": "dome@clasp",
  "control_machines": ["CLASPTCS"],
  "serial_port": "/dev/dome",
  "serial_baud": 9600,
  "serial_timeout": 3,
  "command_delay": 0.25,
  "step_command_delay": 0.25,
  "shutter_timeout": 19,
  "has_legacy_controller": false,
  "has_bumper_guard": false,
  "slow_open_steps": 0,
  "heartbeat_port": "/dev/dome-monitor",
  "heartbeat_baud": 9600,
  "heartbeat_timeout": 3,
  "sides": {
    "north": "a",
    "south": "b",
    "both": "ab"
  },
  "side_labels": {
    "a": "North",
    "b": "South"
  },
  "invert_on_close": false,
  "domealert_daemon": "clasp_domealert",
  "domealert_belt_sensors": {
    "a": "north_belt_tensioned",
    "b": "south_belt_tensioned"
  }
}
